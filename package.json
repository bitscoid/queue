{
  "name": "billing",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "seed": "node prisma/seed.cjs",
    "reset": "rm -rf node_modules && rm -rf .svelte-kit && pnpm install",
    "db:reset": "npx prisma migrate reset --force",
    "start": "pnpm dev",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "prisma": {
    "seed": "node prisma/seed.cjs"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "prisma": "^6.12.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "@tailwindcss/oxide",
      "bcrypt",
      "esbuild",
      "prisma"
    ]
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "@tailwindcss/vite": "^4.1.11",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cookie": "^1.0.2",
    "daisyui": "^5.0.46",
    "jsonwebtoken": "^9.0.2",
    "lucide-svelte": "^0.525.0",
    "tailwindcss": "^4.1.11"
  }
}
